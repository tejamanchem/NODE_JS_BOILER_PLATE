{
  "name": "@Acp/server-core",
  "version": "3.2.0",
  "description": "A delightful way to building a Node.js RESTful API Services with beautiful code written in TypeScript",
  "main": "src/app.ts",
  "scripts": {
    "start": "nps",
    "test": "npm start test",
    "build": "npm start build",
    "presetup": "yarn install",
    "setup": "npm start config && npm start setup.script",
    "clean": "rimraf dist"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/test/preprocessor.js"
    },
    "testTimeout": 70000,
    "globalSetup": "./test/unit/global/globalSetUp.ts",
    "globalTeardown": "./test/unit/global/globalTearDown.ts",
    "setupFilesAfterEnv": [
      "./test/unit/lib/setup.ts"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node"
  },
  "packageManager": "yarn@3.2.3",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/faker": "^6.6.9",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.0.2",
    "@types/morgan": "^1.9.3",
    "@types/nock": "^11.1.0",
    "@types/node": "^18.7.18",
    "@types/node-fetch": "^2.6.2",
    "@types/pg": "^8.6.5",
    "@types/winston": "^2.4.4",
    "cross-env": "^7.0.3",
    "jest": "^29.0.3",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.3.0",
    "ncp": "^2.0.0",
    "nock": "^13.2.9",
    "ts-jest": "^29.0.1"
  },
  "dependencies": {
    "@Acp/backend-core-module": "workspace:modules/backend/core",
    "chalk": "^2.4.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.2",
    "class-validator-jsonschema": "^3.1.1",
    "compression": "^1.7.4",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.0.2",
    "elastic-apm-node": "^3.38.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "express-status-monitor": "^1.3.4",
    "faker": "^6.6.6",
    "figlet": "^1.5.2",
    "glob": "^8.0.3",
    "helmet": "^6.0.0",
    "jsonfile": "^6.1.0",
    "microframework-w3tec": "^0.6.3",
    "morgan": "^1.10.0",
    "mssql": "^6.2.3",
    "nodemon": "^2.0.19",
    "nps": "^5.10.0",
    "nps-utils": "^1.7.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
